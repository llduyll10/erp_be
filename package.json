{
  "name": "nest-boilerplate",
  "version": "1.0.0",
  "description": "NestJS Boilerplate",
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ]
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "yarn typeorm migration:create",
    "migration:show": "yarn typeorm migration:show -d ./src/database/data-source.ts",
    "db:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs db:create -d ./src/database/data-source.ts",
    "db:drop": "yarn typeorm schema:drop -d ./src/database/data-source.ts",
    "db:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/database/typeorm-cli.ts src/database/migrations/Migration",
    "db:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/database/typeorm-cli.ts",
    "db:rollback": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/database/typeorm-cli.ts",
    "db:create:run": "yarn db:create && yarn db:run",
    "db:seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d ./src/database/data-source.ts",
    "db:init": "yarn db:create && yarn db:run",
    "db:reset": "yarn db:drop && yarn db:create && yarn db:run && yarn db:seed",
    "maildev": "maildev --smtp 1025 --web 1080",
    "dev": "concurrently \"yarn start:dev\" \"yarn maildev\""
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@codebrew/nestjs-storage": "^0.1.7",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/throttler": "^5.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@slynova/flydrive": "^1.0.3",
    "@slynova/flydrive-s3": "^1.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.8",
    "@types/passport-jwt": "^3.0.8",
    "@types/redis": "^4.0.11",
    "@types/uuid": "^9.0.0",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.4",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "md5-file": "^5.0.0",
    "multer": "^1.4.5-lts.1",
    "nestjs-form-data": "^1.9.93",
    "nestjs-i18n": "^10.3.1",
    "nestjs-pino": "^3.5.0",
    "nodemailer": "^6.9.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.0",
    "pg-connection-string": "^2.6.1",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.0",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.1",
    "typeorm": "^0.3.0",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.16",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "maildev": "^2.2.1",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
