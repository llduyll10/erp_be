# Cài đặt dependencies chính
yarn add @nestjs/config @nestjs/typeorm typeorm pg class-validator class-transformer @nestjs/swagger @nestjs/jwt @nestjs/passport passport passport-jwt @nestjs/cache-manager cache-manager @nestjs/bull bull @nestjs/platform-express multer nestjs-pino pino-http

# Cài đặt dev dependencies
yarn add -D @types/passport-jwt @types/multer eslint prettier jest ts-jest @types/jest 